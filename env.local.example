# RHOAI Dashboard Development Environment Configuration
# Copy this file to .env.local and customize as needed

# Enable local Kubernetes development with KWOK
LOCAL_K8S=true
KWOK_API_SERVER=http://kwok-cluster:8080
OC_PROJECT=opendatahub

# Development settings
APP_ENV=development
LOG_LEVEL=info

# Disable OpenShift-specific checks for local development
DISABLE_CLUSTER_VERSION_CHECK=true
DISABLE_CONSOLE_CONFIG_CHECK=true

# Optional: Override backend port (default: 8080)
# BACKEND_PORT=8080

# Optional: Override frontend port (default: 4010)
# FRONTEND_PORT=4010

# Optional: Enable additional debugging
# DEBUG=true
